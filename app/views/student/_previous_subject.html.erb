
<span class="span-sub-heading"><%= t 'Subject_details' %></span>
<% remote_form_for :student_previous_subject_details, :url =>{:action =>'save_previous_subject'} do |s| %>
<%= s.hidden_field :student_id, :value => @student.id %>
<div class="label-field-pair">
  <label><%= t 'Subjects' %></label>
  <div class="text-input-bg"><%= s.text_field :subject %></div>
  <div class="extender"></div>
</div>
<div class="label-field-pair">
  <label><%= t 'Marks' %></label>
  <div class="text-input-bg"><%= s.text_field :mark %></div>
  <div class="extender"></div>
</div>
<div class="save-proceed-button" id="save-proceed-button">
      <%= s.submit "", :value => "► 保存"%>
    </div>

<% end %>


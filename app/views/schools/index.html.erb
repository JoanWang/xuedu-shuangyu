<div id="content-header">
    <img src="/images/show_settings.png" alt="Schools" />
  <h1><%= t 'School' %></h1>
  <h3><%= t 'Home' %></h3>
  <div id="app-back-button">
    <%= link_to_function image_tag("/images/buttons/back.png",:border => 0), "history.back()" %>
  </div>
</div>

<div id="page-yield">

  <% unless flash[:notice].nil? %>
    <p class="flash-msg"> <%= flash[:notice] %> </p>
  <% end %>

  <div class="box">

    <div class="link-box">
      <div class="link-heading"><%= link_to t('Manage_compuse'), :action => 'manage_compus' %> </div>
      <div class="link-descr"><%= t 'Manage_compuse_and_classroom' %></div>
    </div>
    <div class="link-box">
      <div class="link-heading"><%= link_to t('Manage_classroom'),:action=>'manage_classrooms' %>  </div>
      <div class="link-descr"><%= t 'Manage_active_classroom'%></div>
    </div>

    <div class="extender"></div>
  </div>
</div>
